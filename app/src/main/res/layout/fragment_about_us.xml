<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.settings.ChangePasswordFragment"
    android:orientation="vertical">

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/settings_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/ToolbarCentered"
        tools:title="Colaboradores" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingStart="16dp"
        android:paddingEnd="16dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Josué Humberto Reyes Suárez"
            android:textColor="@color/gray"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            style="@style/TextAppearance.MaterialComponents.Body1"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Diego Humberto Álvarez Guzman"
            android:textColor="@color/gray"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            style="@style/TextAppearance.MaterialComponents.Body1"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Angel Eduardo Díaz Villarreal"
            android:textColor="@color/gray"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            style="@style/TextAppearance.MaterialComponents.Body1"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Kevin Verdiguel Flores"
            android:textColor="@color/gray"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            style="@style/TextAppearance.MaterialComponents.Body1"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Adrian Ordaz Ruiz"
            android:textColor="@color/gray"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            style="@style/TextAppearance.MaterialComponents.Body1"/>

    </LinearLayout>


</LinearLayout>